{
  "name": "touch",
  "version": "1.0.0",
  "description": "This is a project only for purpose training",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ../node_modules/mocha/bin/mocha test/**/*.js && exit $?",
    "test:watch": "NODE_ENV=test ../node_modules/nodemon/bin/nodemon.js --watch ./test --exec '../node_modules/mocha/bin/mocha test/**/*.js'",
    "test:coverage": "NODE_ENV=test istanbul cover ../node_modules/mocha/bin/_mocha test/**/*.js",
    "start": "npm run apidoc && npm run clusters",
    "dev": "../node_modules/nodemon/bin/nodemon.js --ignore ./test clusters.js --exec '../node_modules/babel-cli/bin/babel-node.js'",
    "clusters": "../node_modules/babel-cli/bin/babel-node.js clusters.js",
    "prod": "NODE_ENV=production npm run clusters",
    "apidoc": "apidoc -i routes/ -o public/apidoc",
    "db:init": "sequelize init",
    "db:migrate": "sequelize db:migrate",
    "db:migration:create": "sequelize migration:create",
    "db:migration:create:test": "NODE_ENV=test npm run db:migration:create",
    "db:migrate:test": "NODE_ENV=test npm run db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "https://hoang-nguyencong@gitlab.asoft-python.com/g-hoangnguyen/nodejs-training.git"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "Hoang Nguyen",
  "license": "ISC",
  "apidoc": {
    "name": "Touch API - Documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "consign": "^0.1.4",
    "express": "^4.15.2",
    "express-validator": "^3.2.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.5",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0"
  }
}
